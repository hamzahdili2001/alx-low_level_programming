#include <stdlib.h>
#include <time.h>
#include <sdtio.h>
/* more headers goes there */
/**
 * main - runs the program
 * Description: get the last digit of number
 * Return: 0
*/
/* betty style doc for function main goes there */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	if (n > 0)
	{
		if (n % 10 > 5)
			printf("Last digit of %d is %d and is greater than
 5\n")
	}
	return (0);
}